#ifndef QUEUE_H
#define QUEUE_H
#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <GenericField.h>
#endif

struct N{
  GenericField *value;
  struct N *next;
} N;

typedef struct N Node;

typedef struct {
  Node *first;
  Node *last;
} Queue;

Node *createNode(GenericField *f,Node *nex);
Queue *createQueue();
bool insertElement(Queue *q, GenericField *f);
bool nextElement(Queue *q);
GenericField *getElementValue(Queue *q);
bool clearQueue(Queue *q);